{"_id":"PtXIF5FyKQJ2i1uM","name":"Carga I","permission":{"default":3,"RRYN2rP4cjdJr3GI":3},"type":"cItem","data":{"ciKey":"","isUnique":false,"uniqueGID":"","usetype":"CON","selfdestruct":false,"effectpath":"","ispermanent":false,"maxuses":"0","icon":"BOOK","rechargable":false,"roll":"","rollid":"mirando","rollname":"#{name} Dano","mods":[],"groups":[{"id":"5VAWTCkJaEl3LPBf","name":"Habilidades de Ação","ikey":"hab_açao_grupo"}],"attributes":{"name":"Itens","alvo":{"value":"Único"},"alcance":{"value":"20"},"lp_custo":{"value":"50"},"limit_cos":{"value":"0"},"ct":{"value":"4"},"mp_custo":{"value":"12"},"base_dano":{"value":"1.25"},"descriçao":{"value":""},"hb_status":{"value":"Nenhum"},"hb_duraçao":{"value":"∞"},"hb_sos":{"value":"0"},"cds":{"value":""},"duracao":{"value":"∞"},"status":{"value":"Nenhum"},"info":{"value":""},"dano_base":{"value":"1.25"},"dmg_code":{"value":"@{dano}+@{dado_dano}"},"atk_code":{"value":""},"atk_tipo":{"value":"Dano Físico"}},"description":"<p style=\"box-sizing: border-box; user-select: text; color: #0c0c0c; font-family: Taviraj, serif;\"><span style=\"box-sizing: border-box; user-select: text;\"><span style=\"box-sizing: border-box; user-select: text;\"><strong style=\"box-sizing: border-box; user-select: text;\">Carga [N&iacute;vel 8]</strong></span></span></p>\n<p style=\"box-sizing: border-box; user-select: text; color: #0c0c0c; font-family: Taviraj, serif;\"><strong style=\"box-sizing: border-box; user-select: text; color: #000000; font-family: Signika, sans-serif;\">Tipo:</strong><span style=\"box-sizing: border-box; user-select: text; color: #000000; font-family: Signika, sans-serif;\">&nbsp;Dano (F&iacute;sico) / A&ccedil;&atilde;o lenta&nbsp;</span><strong style=\"box-sizing: border-box; user-select: text; color: #000000; font-family: Signika, sans-serif;\">(4) - Alvo: </strong><span style=\"box-sizing: border-box; user-select: text; color: #000000; font-family: Signika, sans-serif;\">&Uacute;nico</span></p>\n<p style=\"box-sizi